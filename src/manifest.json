{
    "manifest_version": 2,

    "name": "Adressaway",
    "description": "Enhances user experience on adressa.no",

    "version": "0.1",

    "background": {
        "scripts": ["adressaway.js"]
    },

    "icons": {"48": "img/icon48x48.png", "128": "img/icon128x128.png"},

    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "*://*.adressa.no/"
    ]
}
